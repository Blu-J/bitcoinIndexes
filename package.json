{
  "name": "shapeshiftInterview",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "install": "(cd client; yarn) && (cd server; yarn)",
    "client:dev": "(cd client; yarn next)",
    "client:build": "(cd client; yarn next build)",
    "client:start": "(cd client; yarn next build && yarn next start)",
    "proxy": "yarn wait-on tcp:3000 && yarn wait-on tcp:4000 && micro-proxy -r proxyRules.json -p $PORT",
    "server:build": "(cd server; yarn tsc)",
    "server:start": "yarn server:build && node ./server/dist/index.js",
    "server:dev": "concurrently '(cd server; yarn tsc -w)' '(cd server; yarn nodemon yarn server:start)'",
    "all": "concurrently 'yarn client:start' 'yarn proxy' 'yarn server:start'",
    "all:dev": "concurrently 'yarn client:dev' 'yarn proxy' 'yarn server:dev'",
    "web": "yarn all"
  },
  "dependencies": {
    "concurrently": "4.0.1",
    "micro-proxy": "^1.1.0",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {}
}
